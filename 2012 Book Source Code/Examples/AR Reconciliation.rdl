  <Code>Public Function GroupingTotalDisplay(ByVal GroupingValue As String) As String

        Dim SearchCriteria As String = "Billcode"
        Dim SearchText As String = GroupingValue.ToUpper
        Dim index As Integer
        Dim DisplayText As String

        index = SearchText.IndexOf(SearchCriteria.ToUpper)

        If index &gt;= 0 Then
            DisplayText = "Billcode Report Category"
        Else
            index = 0
	    SearchCriteria = "Bill"
	    
	    index = SearchText.IndexOf(SearchCriteria.ToUpper)
	    
	    if index &gt;= 0 Then
		DisplayText = "Payor Bill Category"		
	    Else
		DisplayText = "Payor"
	    End if

        End If

        Return DisplayText

    End Function
</Code>
